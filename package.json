  "devDependencies": {
    "@types/mocha": "^2.2.41",
    "@types/react": "^15.6.0",
    "@types/react-dom": "^15.5.1",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-sauce-launcher": "^1.1.0",
    "karma-webpack": "^2.0.4",
    "mocha": "^3.4.2",
    "mocha-loader": "^1.1.1",
    "ncp": "^2.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "rimraf": "^2.6.1",
    "ts-loader": "^2.3.2",
    "typescript": "^2.4.2",
