{
  "name": "test-drive-react",
  "description": "Opinionated library for Test-Driven Development of React Components.",
  "version": "0.0.11",
  "main": "./dist/src/index.js",
  "mainSrc": "src/index.ts",
  "types": "./dist/src/index.d.ts",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "tsc -d",
    "bundle": "webpack --progress",
    "pretest": "npm run clean && npm run build && npm run bundle",
    "test": "npm run test:node && npm run test:browser",
    "test:node": "mocha --reporter mocha-env-reporter dist/test/index.node.js",
    "test:browser": "karma start --single-run",
    "start": "webpack-dev-server --progress"
  },
  "devDependencies": {
    "@types/chai": "3.4.35",
    "@types/mocha": "2.2.40",
    "@types/sinon": "1.16.36",
    "chai": "3.5.0",
    "karma": "1.5.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-env-reporter": "1.0.13",
    "karma-mocha": "1.3.0",
    "mocha": "3.2.0",
    "mocha-env-reporter": "2.0.4",
    "mocha-loader": "1.1.1",
    "npminject": "1.0.252",
    "rimraf": "2.6.1",
    "typescript": "2.1.6",
    "ts-loader": "2.0.3",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2"
  },
  "dependencies": {
    "@types/react": "15.0.21",
    "@types/react-dom": "0.14.23",
    "@types/react-addons-test-utils": "0.14.17",
    "react": "15.4.2",
    "react-addons-test-utils": "15.4.2",
    "react-dom": "15.4.2",
    "test-drive": "0.0.65"
  },
  "author": "Jiri Tobisek",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wix/test-drive-react.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wix/test-drive-react/issues"
  },
  "homepage": "https://github.com/wix/test-drive-react#readme",
  "keywords": []
}
